{
  "name": "@marcellejs/core",
  "description": "Marcelle Core API",
  "version": "0.4.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcellejs/marcelle"
  },
  "author": {
    "name": "Jules FrancÌ§oise",
    "email": "me@julesfrancoise.com"
  },
  "keywords": [
    "marcelle",
    "interactive machine learning"
  ],
  "license": "MIT",
  "main": "dist/marcelle.bundle.esm.js",
  "module": "dist/marcelle.esm.js",
  "browser": "dist/marcelle.bundle.umd.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/marcelle.bundle.umd.js",
      "import": "./dist/marcelle.esm.js",
      "default": "./dist/marcelle.esm.js"
    },
    "./dist/marcelle.css": "./dist/marcelle.css",
    "./ui": "./dist/ui.js",
    "./ui.css": "./dist/ui.css"
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "build-all": "$npm_execpath run clean && $npm_execpath run build && $npm_execpath run ui:build && $npm_execpath run types:build",
    "examples:dev": "vite --config examples/vite.config.js",
    "examples:build": "vite build --config examples/vite.config.js",
    "types:dev": "tsc --project tsconfig.json  -d --emitDeclarationOnly --outDir types --watch",
    "types:build": "tsc --project tsconfig.json  -d --emitDeclarationOnly --outDir types",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "ui:dev": "rollup -w -c rollup.config.ui.js",
    "ui:build": "rollup -c rollup.config.ui.js",
    "storybook:dev": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "lint": "eslint src",
    "clean": "rm -rf dist types examples/dist"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.2.1",
    "@storybook/addon-a11y": "^6.3.0-alpha.34",
    "@storybook/addon-actions": "^6.3.0-alpha.21",
    "@storybook/addon-essentials": "^6.3.0-alpha.21",
    "@storybook/addon-links": "^6.3.0-alpha.21",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-svelte-csf": "^1.0.0",
    "@storybook/svelte": "^6.3.0-alpha.21",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.11",
    "@types/blueimp-load-image": "^5.14.2",
    "@types/lodash": "^4.14.168",
    "@types/ndarray": "^1.0.9",
    "@types/ndarray-ops": "^1.2.1",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "ejs": "^3.1.6",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "postcss": "^8.2.14",
    "postcss-load-config": "^3.0.1",
    "prettier": "^2.2.0",
    "rollup": "^2.48.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-sizes": "^1.0.4",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "3.39.0",
    "svelte-loader": "^3.1.1",
    "svelte-preprocess": "^4.7.3",
    "svelte-range-slider-pips": "^1.7.0",
    "tailwindcss": "^2.1.4",
    "typescript": "^4.3.2",
    "vite": "^2.3.7",
    "vuepress": "^1.8.2"
  },
  "dependencies": {
    "@feathersjs/authentication-client": "^4.5.11",
    "@feathersjs/client": "^4.5.11",
    "@feathersjs/feathers": "^4.5.11",
    "@feathersjs/socketio-client": "^4.5.11",
    "@most/adapter": "^1.0.0",
    "@most/core": "^1.6.1",
    "@most/scheduler": "^1.3.0",
    "@most/types": "^1.1.0",
    "@tensorflow-models/coco-ssd": "^2.2.2",
    "@tensorflow-models/knn-classifier": "^1.2.2",
    "@tensorflow-models/mobilenet": "^2.1.0",
    "@tensorflow/tfjs-backend-cpu": "^3.7.0",
    "@tensorflow/tfjs-backend-webgl": "^3.7.0",
    "@tensorflow/tfjs-converter": "^3.7.0",
    "@tensorflow/tfjs-core": "^3.7.0",
    "@tensorflow/tfjs-layers": "^3.7.0",
    "blueimp-load-image": "^5.14.0",
    "chart.js": "^3.3.0",
    "chartjs-chart-matrix": "^1.0.0",
    "date-fns": "^2.22.1",
    "dequal": "^2.0.2",
    "feathers-localstorage": "^5.2.0",
    "feathers-memory": "^4.1.0",
    "lodash": "^4.17.21",
    "ml-kmeans": "^5.0.0",
    "scatter-gl": "^0.0.8",
    "socket.io-client": "^2.4.0"
  }
}
