    "rollup": "^2.48.0",
    "rollup-plugin-external-globals": "^0.6.1",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-progress": "^1.1.2",
