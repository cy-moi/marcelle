{
  "name": "@marcellejs/design-system",
  "version": "0.5.1",
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "pnpm run package",
    "package": "svelte-kit package && rm dist/package.json&& rm dist/README.md",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "clean": "rm -rf dist"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@feathersjs/feathers": "^4.5.11",
    "@fontsource/fira-mono": "^4.5.0",
    "@lukeed/uuid": "^2.0.0",
    "cookie": "^0.4.1",
    "date-fns": "^2.25.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^1.0.0-next.31",
    "@sveltejs/kit": "^1.0.0-next.295",
    "@types/cookie": "^0.4.1",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "postcss": "^8.2.14",
    "postcss-load-config": "^3.1.0",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.5",
    "svelte-preprocess": "^4.10.4",
    "svelte2tsx": "^0.4.8",
    "tailwindcss": "^3.0.23",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
  },
  "exports": {
    "./package.json": "./package.json",
    "./Button.svelte": "./dist/Button.svelte",
    "./Input.svelte": "./dist/Input.svelte",
    "./Modal.svelte": "./dist/Modal.svelte",
    "./Notification.svelte": "./dist/Notification.svelte",
    "./Number.svelte": "./dist/Number.svelte",
    "./NumberArray.svelte": "./dist/NumberArray.svelte",
    "./PopMenu.svelte": "./dist/PopMenu.svelte",
